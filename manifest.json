{
    "name": "Google Ads Search Terms Wrapper",
    "version": "1.1",
    "description": "Wrap search terms in the Google Ads Search Terms report",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "favicon16.png",
            "32": "favicon32.png"
        }
    },
    "icons": {
        "16": "favicon16.png",
        "32": "favicon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}  